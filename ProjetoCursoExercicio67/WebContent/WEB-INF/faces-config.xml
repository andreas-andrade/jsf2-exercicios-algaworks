<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
    <application>
    	<message-bundle>com.algaworks.cursojsf2.financeiro.resources.Messages</message-bundle>
    </application>

	<navigation-rule>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/Home.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>consulta-pessoas</from-outcome>
			<to-view-id>consulta-pessoas.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
    
		<navigation-case>
			<from-outcome>cadastro_lancamento</from-outcome>
			<to-view-id>CadastroLancamento.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>cadastro_pessoa</from-outcome>
			<to-view-id>cadastro-pessoa.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>consulta_pessoa</from-outcome>
			<to-view-id>consulta-pessoas.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>consulta_lancamento</from-outcome>
			<to-view-id>ConsultaLancamento.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
    </faces-config>

